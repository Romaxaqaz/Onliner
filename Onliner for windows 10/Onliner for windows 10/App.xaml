<common:BootStrapper
    x:Class="Onliner_for_windows_10.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:common="using:Template10.Common"
    RequestedTheme="Light">


    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/ThemeStyle.xaml"/>
                <ResourceDictionary Source="Themes/Generic.xaml"/>
                <ResourceDictionary Source="Themes/ListViewThemeStyle.xaml"/>
                <ResourceDictionary Source="Themes/ContolsStyle.xaml"/>
                <ResourceDictionary Source="Themes/StringResource.xaml"/>
                <ResourceDictionary Source="Themes/DataTemplateStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</common:BootStrapper>
